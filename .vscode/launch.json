{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: PyShell",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "PYSHELL_CRYPT_KEY": "MyMinimum30CharacterAESKey1234",
                "PYSHELL_HOST": "0.0.0.0",
                "PYSHELL_PORT": "5050",
                "PYSHELL_PROC_TIMEOUT": "1800"
            },
            "program": "${workspaceFolder}/pyshell.py",
            "console": "integratedTerminal"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch JS Client - ls command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {"MONKSHU_HOME": "/Users/rvkapoor/Documents/Programs/source/monkshu"},
            "program": "${workspaceFolder}/pyshellclient.js",
            "args": [
                "-h",
                "127.0.0.1",
                "-p",
                "5050",
                "-k",
                "MyMinimum30CharacterAESKey1234",
                "-m",
                "ls",
                "-al"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch JS Client Polling Mode - ls command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {"MONKSHU_HOME": "/Users/rvkapoor/Documents/Programs/source/monkshu"},
            "program": "${workspaceFolder}/pyshellclient.js",
            "args": [
                "-h",
                "127.0.0.1",
                "-p",
                "5050",
                "-k",
                "MyMinimum30CharacterAESKey1234",
                "-r",
                "500",
                "-m",
                "ls",
                "-al"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch JS Client - script",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {"MONKSHU_HOME": "/Users/rvkapoor/Documents/Programs/source/monkshu"},
            "program": "${workspaceFolder}/pyshellclient.js",
            "args": [
                "-h",
                "127.0.0.1",
                "-p",
                "5050",
                "-k",
                "MyMinimum30CharacterAESKey1234",
                "-s",
                "${workspaceFolder}/test.sh",
                "ls",
                "/Users/"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch JS Client Polling Mode - script",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {"MONKSHU_HOME": "/Users/rvkapoor/Documents/Programs/source/monkshu"},
            "program": "${workspaceFolder}/pyshellclient.js",
            "args": [
                "-h",
                "127.0.0.1",
                "-p",
                "5050",
                "-k",
                "MyMinimum30CharacterAESKey1234",
                "-r",
                "500",
                "-s",
                "${workspaceFolder}/test.sh",
                "ls",
                "/Users/"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch JS Client - health command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {"MONKSHU_HOME": "/Users/rvkapoor/Documents/Programs/source/monkshu"},
            "program": "${workspaceFolder}/pyshellclient.js",
            "args": [
                "-h",
                "127.0.0.1",
                "-p",
                "5050",
                "-k",
                "MyMinimum30CharacterAESKey1234",
                "-t"
            ]
        }
    ]
}